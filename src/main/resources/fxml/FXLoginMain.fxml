<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="fx.controller.FXLoginMainController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <TextField fx:id="fxUser" layoutX="244.0" layoutY="183.0" />
        <TextField fx:id="passBox" accessibleRole="PASSWORD_FIELD" layoutX="244.0" layoutY="215.0" />
        <Label layoutX="163.0" layoutY="186.0" prefWidth="60.0" text="User" />
        <Label layoutX="163.0" layoutY="219.0" text="Password" />
        <Label alignment="CENTER" layoutX="234.0" layoutY="76.0" text="Welcome" textFill="#000000ae">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
        <Button defaultButton="true" layoutX="241.0" layoutY="280.0" mnemonicParsing="false" onAction="#clickLogin" prefHeight="25.0" prefWidth="149.0" text="Login" />
        <Label fx:id="errorBox" layoutX="241.0" layoutY="252.0" prefHeight="17.0" prefWidth="0.0" textFill="#ff4848" />
    </children>

</AnchorPane>
