<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="fx.controller.spring.FXDeleteNewspaperSpringController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <Label layoutX="203.0" layoutY="68.0" text="Delete Newspaper" textFill="#000000d9">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label layoutX="105.0" layoutY="103.0" text="Please, select the newspaper you want to remove from the database" textFill="#000000be">
            <font>
                <Font size="13.0" />
            </font></Label>
        <Button layoutX="47.0" layoutY="329.0" mnemonicParsing="false" onAction="#deleteNewspaper" prefHeight="25.0" prefWidth="519.0" text="DELETE" textFill="#a41818">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Button>
        <ListView fx:id="newspaperList" layoutX="47.0" layoutY="123.0" prefHeight="200.0" prefWidth="519.0" />
    </children>

</AnchorPane>
