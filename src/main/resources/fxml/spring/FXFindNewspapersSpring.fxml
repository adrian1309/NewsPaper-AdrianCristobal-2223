<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="fx.controller.spring.FXFindNewspapersSpringController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <TableView fx:id="newspaperTable" layoutX="29.0" layoutY="89.0" prefHeight="280.0" prefWidth="350.0">
            <columns>
                <TableColumn fx:id="idColumnTable" prefWidth="50" text="ID" />
                <TableColumn fx:id="nameColumnTable" prefWidth="150" text="NAME" />
                <TableColumn fx:id="releaseDateColumnTable" prefWidth="150.0" text="RELEASE DATE" />
            </columns>
        </TableView>
        <Label layoutX="36.0" layoutY="41.0" prefHeight="37.0" prefWidth="168.0" text="NEWSPAPER LIST">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
    </children>

</AnchorPane>
